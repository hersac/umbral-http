    pu f: serviceUnavailable(data, customMessage->Str)->Response {
        c: msg = i: (customMessage != nulo) { r: (customMessage); } e: { r: ("Service Unavailable"); };
        r: (n: Response(503, msg, data));
    }

    pu f: gatewayTimeout(data, customMessage->Str)->Response {
        c: msg = i: (customMessage != nulo) { r: (customMessage); } e: { r: ("Gateway Timeout"); };
