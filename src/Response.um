out cs: Response {
    pr: code->Int;
    pr: message->Str;
    pr: data;

    pu f: Response(code->Int, message->Str, data) {
        th.code = code;
        th.message = message;
        th.data = data;
    }

    pu f: toJson() {
        r: (["code"=>th.code, "message"=>th.message, "data"=>th.data]);
    }

    pu f: ok(data, customMessage->Str)->Response {
        c: msg = i: (customMessage != nulo) { r: (customMessage); } e: { r: ("OK"); };
        r: (n: Response(200, msg, data));
    }

    pu f: created(data, customMessage->Str)->Response {
        c: msg = i: (customMessage != nulo) { r: (customMessage); } e: { r: ("Created"); };
