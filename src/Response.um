        r: (n: Response(403, msg, data));
    }

    pu f: notFound(data, customMessage->Str)->Response {
        c: msg = i: (customMessage != nulo) { r: (customMessage); } e: { r: ("Not Found"); };
        r: (n: Response(404, msg, data));
    }

    pu f: methodNotAllowed(data, customMessage->Str)->Response {
        c: msg = i: (customMessage != nulo) { r: (customMessage); } e: { r: ("Method Not Allowed"); };
        r: (n: Response(405, msg, data));
    }

    pu f: conflict(data, customMessage->Str)->Response {
        c: msg = i: (customMessage != nulo) { r: (customMessage); } e: { r: ("Conflict"); };
        r: (n: Response(409, msg, data));
    }

    pu f: internalServerError(data, customMessage->Str)->Response {
        c: msg = i: (customMessage != nulo) { r: (customMessage); } e: { r: ("Internal Server Error"); };
