    }

    pu f: noContent(customMessage->Str)->Response {
        c: msg = i: (customMessage != nulo) { r: (customMessage); } e: { r: ("No Content"); };
        r: (n: Response(204, msg, nulo));
    }

    pu f: badRequest(data, customMessage->Str)->Response {
        c: msg = i: (customMessage != nulo) { r: (customMessage); } e: { r: ("Bad Request"); };
        r: (n: Response(400, msg, data));
